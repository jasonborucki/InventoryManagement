<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="502.0" prefWidth="400.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.ModifyPart">
   <children>
      <Label layoutX="159.0" layoutY="58.0" text="Modify Part">
         <font>
            <Font size="22.0" />
         </font>
      </Label>
      <RadioButton fx:id="inHouseButton" layoutX="98.0" layoutY="102.0" mnemonicParsing="false" onAction="#onClickInHouse" selected="true" text="In-House">
         <toggleGroup>
            <ToggleGroup fx:id="group1" />
         </toggleGroup></RadioButton>
      <RadioButton fx:id="outsourcedButton" layoutX="241.0" layoutY="102.0" mnemonicParsing="false" onAction="#onClickOutsourced" text="Outsourced" toggleGroup="$group1" />
      <TextField fx:id="modPartName" layoutX="126.0" layoutY="203.0" />
      <TextField fx:id="modPartInv" layoutX="126.0" layoutY="245.0" />
      <TextField fx:id="modPartCost" layoutX="126.0" layoutY="287.0" />
      <TextField fx:id="modPartMin" layoutX="126.0" layoutY="329.0" />
      <TextField fx:id="modPartMax" layoutX="126.0" layoutY="371.0" />
