      <Label layoutX="131.0" layoutY="186.0" text="Name" />
      <Label layoutX="133.0" layoutY="228.0" text="Inv" />
      <Label layoutX="131.0" layoutY="270.0" text="Price/Cost" />
      <Label layoutX="131.0" layoutY="312.0" text="Min" />
      <Label layoutX="130.0" layoutY="354.0" text="Max" />
      <Label fx:id="machineIdLabel" layoutX="128.0" layoutY="396.0" text="Machine ID" />
      <Button layoutX="115.0" layoutY="460.0" mnemonicParsing="false" onAction="#onClickSave" prefHeight="25.0" prefWidth="52.0" text="Save" />
      <Button layoutX="241.0" layoutY="460.0" mnemonicParsing="false" onAction="#onClickCancel" text="Cancel" />
      <TextField fx:id="modPartId" disable="true" editable="false" layoutX="127.0" layoutY="161.0" text="Auto-generated ID" />
      <Label layoutX="135.0" layoutY="138.0" text="ID" />
   </children>
</AnchorPane>
